<div class="col feedItem mb-3">
    <div class="row align-items-center">
        <div class="col-2 col-sm-2 py-3">
            <img src="/images/icon.png" alt="user profile photo" class="img-fluid">
        </div>
        <div class="col-8">
            <h4 class="feedName">{{user}}</h4>
            <p class="feedStatus">{{messageBody}}</p>
            <ul class="list-inline">
                <li class="list-inline-item feedOptions">Comment</li>
                <li class="list-inline-item feedOptions">Follow this post</li>
            </ul>
        </div>
    </div>
</div>
